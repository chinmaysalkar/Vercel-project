<%- contentFor('HeaderCss') %>
    <!-- jsvectormap css -->
    <link href="<%= process.env.AAP_URL %>assets/libs/jsvectormap/css/jsvectormap.min.css" rel="stylesheet"
        type="text/css" />

    <!--Swiper slider css-->
    <link href="<%= process.env.AAP_URL %>assets/libs/swiper/swiper-bundle.min.css" rel="stylesheet" type="text/css" />
    <%- contentFor('body') %>

        <div class="row">
            <div class="col">

                <div class="h-100">
