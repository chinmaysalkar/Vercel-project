            <!-- prismjs plugin -->
            <script src="<%= process.env.AAP_URL %>assets/js/jquery/jquery-3.6.0.min.js"></script>

            <script src="<%= process.env.AAP_URL %>assets/js/jquery/jquery.validate.min.js"></script>
