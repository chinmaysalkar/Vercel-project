<!doctype html>
<html lang="en" data-layout="vertical" data-topbar="light" data-sidebar="dark" data-sidebar-size="lg">

<head>

    <meta charset="utf-8" />
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
    <meta content="Themesbrand" name="author" />
    <!-- App favicon -->
    <link rel="shortcut icon" href="<%= process.env.AAP_URL %>assets/images/favicon.ico">

    <!-- Layout config Js -->
    <script src="<%= process.env.AAP_URL %>assets/js/layout.js"></script>
    <!-- Bootstrap Css -->
    <link href="<%= process.env.AAP_URL %>assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="<%= process.env.AAP_URL %>assets/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="<%= process.env.AAP_URL %>assets/css/app.min.css" id="app-style" rel="stylesheet" type="text/css" />
    <!-- custom Css-->
    <link href="<%= process.env.AAP_URL %>assets/css/custom.min.css" id="app-style" rel="stylesheet" type="text/css" />

</head>

<body>

<div class="auth-page-wrapper pt-5">
    <!-- auth page bg -->
    <div class="auth-one-bg-position auth-one-bg"  id="auth-particles">
        <div class="bg-overlay"></div>
        
        <div class="shape">
            <svg xmlns="http://www.w3.org/2000/svg" version="1.1" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 1440 120">
                <path d="M 0,36 C 144,53.6 432,123.2 720,124 C 1008,124.8 1296,56.8 1440,40L1440 140L0 140z"></path>
            </svg>
        </div>
    </div>

    <!-- auth page content -->
    <div class="auth-page-content">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="text-center mt-sm-5 mb-4 text-white-50">
                        <div>
                            <a href="<%= process.env.AAP_URL %>signup" class="d-inline-block auth-logo">
                                <img src="<%= process.env.AAP_URL %>assets/images/logo.jpg" alt="" height="55">
                            </a>
                        </div>
                        <!-- <p class="mt-3 fs-15 fw-medium">Premium Admin & Dashboard Template</p> -->
                    </div>
                </div>
            </div>
            <!-- end row -->

            <div class="row justify-content-center">
                <div class="col-md-8 col-lg-6 col-xl-5">
                    <div class="card mt-4">
                        <div class="card-body p-4 text-center"> 
                            <div class="avatar-lg mx-auto mt-2">
                                <div class="avatar-title bg-light text-success display-3 rounded-circle">
                                    <i class="ri-checkbox-circle-fill"></i>
                                </div>
                            </div>
                            <div class="mt-4 pt-2">
                                <h4>Well done !</h4>
                                <p class="text-muted mx-4">Congratulations, your registration has been successfully.</p>
                                <!-- <div class="mt-4">
                                    <a href="<%= process.env.AAP_URL %>login" class="btn btn-success w-100">Back to Login</a>
                                </div> -->
                            </div>
                        </div>
                        <!-- end card body -->
                    </div>
                    <!-- end card -->

                </div>
            </div>
            <!-- end row -->
        </div>
        <!-- end container -->
    </div>
    <!-- end auth page content -->

    <!-- footer -->
    <footer class="footer">
        <!-- <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="text-center">
                        <p class="mb-0 text-muted">&copy; <script>document.write(new Date().getFullYear())</script> Velzon. Crafted with <i class="mdi mdi-heart text-danger"></i> by Themesbrand</p>
                    </div>
                </div>
            </div>
        </div> -->
    </footer>
    <!-- end Footer -->
</div>
<!-- end auth-page-wrapper -->
 
<!-- particles js -->
<script src="<%= process.env.AAP_URL %>assets/libs/particles.js/particles.js"></script>
<!-- particles app js -->
<script src="<%= process.env.AAP_URL %>assets/js/pages/particles.app.js"></script>
<!-- password-addon init -->
<script src="<%= process.env.AAP_URL %>assets/js/pages/password-addon.init.js"></script>

    <!-- App js -->
    <script src="<%= process.env.AAP_URL %>assets/js/app.js"></script>
</body>

</html>
